    "eslint": "eslint-godaddy-react src/ test/",
    "unit": "mocha --require setup-env \"test/unit/**/*.tests.js\"",
    "posttest": "npm run eslint",
    "test": "nyc --reporter=text --reporter=json-summary npm run unit",
